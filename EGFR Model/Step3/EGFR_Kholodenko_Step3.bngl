begin seed species
  EGF(egfr)                     680   #nM
  EGFR(egf,egfr,p~0,shc,grb2)   100.0 #nM
  Shc(egfr,p~0,grb2)            150.0 #nM
  Grb2(egfr,sos,shc)            85.0  #nM
  SOS(grb2)                     34.0  #nM
end seed species
