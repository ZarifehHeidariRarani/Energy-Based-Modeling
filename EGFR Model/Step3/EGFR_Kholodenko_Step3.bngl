begin functions                                              
  fRate3()  = r4_V4 /(EGF_EGFR2_P +r4_K4)
  fRate15() = r16_V16 /(Shc_P+r16_K16)
end functions

begin reaction rules
  v1:  EGFR(egfr,egf,p~0,shc,grb2) + EGF(egfr) <-> EGFR(egfr,egf!1,p~0,shc,grb2).EGF(egfr!1) r1_k1f,r1_k1b
  v2:  EGFR(egfr,egf!1,p~0,shc,grb2).EGF(egfr!1) + EGFR(egfr,egf!1,p~0,shc,grb2).EGF(egfr!1) <-> EGFR(egfr!1,egf,p~0,shc,grb2).EGFR(egfr!1,egf,p~0,shc,grb2) r2_k2f,r2_k2b
  v3:  EGFR(egfr!1,egf,p~0,shc,grb2).EGFR(egfr!1,egf,p~0,shc,grb2) <-> EGFR(egfr,egf!1,p~P,shc,grb2).EGF(egfr!1) r3_k3f,r3_k3b
  v4:  EGFR(egfr,egf!1,p~P,shc,grb2).EGF(egfr!1) -> EGFR(egfr!1,egf,p~0,shc,grb2).EGFR(egfr!1,egf,p~0,shc,grb2) fRate3
  v9:  Grb2(egfr,sos,shc) + EGFR(egfr,egf!1,grb2,p~P,shc).EGF(egfr!1) <-> EGFR(egfr,egf,grb2!1,p~0,shc).Grb2(egfr!1,sos,shc) r9_k9f,r9_k9b
  v10: EGFR(egfr,egf,grb2!1,p~0,shc).Grb2(egfr!1,sos,shc) + SOS(grb2) <-> EGFR(egfr!1,egf,grb2,p~0,shc).EGFR(egfr!1,egf,grb2!2,p~0,shc).Grb2(egfr!2,sos!3,shc).SOS(grb2!3) r10_k10f,r10_k10b
  v11: EGFR(egfr!1,egf,grb2,p~0,shc).EGFR(egfr!1,egf,grb2!2,p~0,shc).Grb2(egfr!2,sos!3,shc).SOS(grb2!3) <-> Grb2(egfr,sos!1,shc).SOS(grb2!1) + EGFR(egfr,egf!1,grb2,p~P,shc).EGF(egfr!1) r11_k11f,r11_k11b
  v12: Grb2(egfr,sos!1,shc).SOS(grb2!1) <-> Grb2(egfr,sos,shc) + SOS(grb2) r12_k12f,r12_k12b
  v13: Shc(egfr,p~0,grb2) + EGFR(egfr,egf!1,p~P,shc,grb2).EGF(egfr!1) <-> EGFR(egfr,egf!1,p~P,shc!2,grb2).Shc(egfr!2,p~0,grb2).EGF(egfr!1) r13_k13f,r13_k13b
  v14: EGFR(egfr,egf!1,p~P,shc!2,grb2).Shc(egfr!2,p~0,grb2).EGF(egfr!1) <-> EGFR(egfr,egf!1,p~0,shc!2,grb2).Shc(egfr!2,p~P,grb2).EGF(egfr!1) r14_k14f,r14_k14b
  v15: EGFR(egfr,egf!1,p~0,shc!2,grb2).Shc(egfr!2,p~P,grb2).EGF(egfr!1) <-> EGFR(egfr,egf!1,p~P,shc,grb2).EGF(egfr!1) + Shc(egfr,p~P,grb2) r15_k15f,r15_k15b
  v16: Shc(egfr,p~P,grb2) -> Shc(egfr,p~0,grb2) fRate15
  v17: EGFR(egfr,egf!1,grb2,p~0,shc!2).Shc(egfr!2,grb2,p~P).EGF(egfr!1) + Grb2(egfr,shc,sos) <-> EGFR(egfr!1,egf,grb2,p~0,shc).EGFR(egfr!1,egf,grb2,p~0,shc!2).Grb2(egfr,shc!3,sos).Shc(egfr!2,grb2!3,p~0) r17_k17f,r17_k17b
  v18: EGFR(egfr!1,egf,grb2,p~0,shc).EGFR(egfr!1,egf,grb2,p~0,shc!2).Grb2(egfr,shc!3,sos).Shc(egfr!2,grb2!3,p~0) <-> Shc(egfr,grb2!1,p~0).Grb2(egfr,shc!1,sos) + EGFR(egfr,egf!1,grb2,p~P,shc).EGF(egfr!1) r18_k18f,r18_k18b
  v19: SOS(grb2) + EGFR(egfr!1,egf,grb2,p~0,shc).EGFR(egfr!1,egf,grb2,p~0,shc!2).Grb2(egfr,shc!3,sos).Shc(egfr!2,grb2!3,p~0) <-> EGFR(egfr!1,egf,grb2,p~0,shc).EGFR(egfr!1,egf,grb2,p~0,shc!2).Grb2(egfr,shc!3,sos!4).Shc(egfr!2,grb2!3,p~0).SOS(grb2!4) r19_k19f,r19_k19b
  v20: EGFR(egfr!1,egf,grb2,p~0,shc).EGFR(egfr!1,egf,grb2,p~0,shc!2).Grb2(egfr,shc!3,sos!4).Shc(egfr!2,grb2!3,p~0).SOS(grb2!4) <-> Grb2(egfr,shc!1,sos!2).Shc(egfr,grb2!1,p~0).SOS(grb2!2) + EGFR(egfr,egf!1,grb2,p~P,shc).EGF(egfr!1) r20_k20f,r20_k20b
  v21: Grb2(egfr,shc,sos) + Shc(egfr,grb2,p~P) <-> Shc(egfr,grb2!1,p~0).Grb2(egfr,shc!1,sos) r21_k21f,r21_k21b
  v22: Shc(egfr,grb2!1,p~0).Grb2(egfr,shc!1,sos) + SOS(grb2) <-> Grb2(egfr,shc!1,sos!2).Shc(egfr,grb2!1,p~0).SOS(grb2!2) r22_k22f,r22_k22b
  v23: Grb2(egfr,shc!1,sos!2).Shc(egfr,grb2!1,p~0).SOS(grb2!2) <-> Grb2(egfr,shc,sos!1).SOS(grb2!1) + Shc(egfr,grb2,p~P) r23_k23f,r23_k23b
  v24: EGFR(egfr,egf!1,grb2,p~0,shc!2).Shc(egfr!2,grb2,p~P).EGF(egfr!1) + Grb2(egfr,shc,sos!1).SOS(grb2!1) <-> EGFR(egfr!1,egf,grb2,p~0,shc).EGFR(egfr!1,egf,grb2,p~0,shc!2).Grb2(egfr,shc!3,sos!4).Shc(egfr!2,grb2!3,p~0).SOS(grb2!4) r24_k24f,r24_k24b
end reaction rules

